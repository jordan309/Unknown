<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jordan307.unknown">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CharacterSelect"
            android:label="@string/title_activity_character_select"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".GameLossFood"
            android:label="@string/title_activity_game_loss_food"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GameLossSanity"
            android:label="@string/title_activity_game_loss_sanity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GameLossWater"
            android:label="@string/title_activity_game_loss_water"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GameWin"
            android:label="@string/title_activity_game_win"
            android:theme="@style/AppTheme.NoActionBar" />

        <service
            android:name=".MusicService"
            android:enabled="true" />

        <activity android:name=".AboutActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".IntroActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>